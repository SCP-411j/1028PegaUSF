package com.example.assignment1;

public class MyClass {
	
	public static void main(String[] args) {
		Bean bean1 = new Bean(10, "white", "bean1");
		Bean bean2 = new Bean(10, "white", "bean2");
		Bean bean3 = new Bean(10, "white", "bean3");
		
		
	
		
		
		Bean[] beanArray = new Bean[3];
		beanArray[0] = bean1;
		beanArray[1] = bean2;
		beanArray[2] = bean3;
		
		for(int i = 0; i<beanArray.length; i++) {
//			beanArray[i] = new Bean();
			System.out.println(beanArray[i].toString());
			
		}
	}
}
