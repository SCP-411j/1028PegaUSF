package com.example.assignment1;

public class MyClass {
	
	public static void main(String[] args) {
		Bean bean1 = new Bean();
		bean1.setColor("red");
		bean1.setName("bean1");
		bean1.setNumOfBeans(15);
		
		Bean bean2 = new Bean();
		bean1.setColor("brown");
		bean1.setName("bean2");
		bean1.setNumOfBeans(20);
		
		Bean bean3 = new Bean();
		bean1.setColor("pink");
		bean1.setName("bean3");
		bean1.setNumOfBeans(25);
	}
}
